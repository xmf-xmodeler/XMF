parserImport XOCL;

context Root  
  
  @Operation getOpValues(object)
    let 
      set = Set{};
      ops = object.of().operations
    in
      @For op in ops do  
        if op.arity = 0 
          and op.name.toString() <> "loopInfinitely" 
        then 
          op.println();     
          let 
            result = op.invoke(object,{})
          in
            result.println();
            let 
              seq = Seq{op, result.toString()}
            in
              set := set.union({seq})
            end
          end
        end
      end;
      set
    end
  end