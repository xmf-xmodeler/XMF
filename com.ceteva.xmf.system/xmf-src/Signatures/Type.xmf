parserImport XOCL;

import Signatures;
import IO;

context Signatures

  @Class Type isabstract
  
    @Operation force():Type
      self
    end
    
    @Operation lookup(path:Seq(String))
      if path->isEmpty
      then self
      else self.error("No binding for " + path->head)
      end
    end
    
    @AbstractOp pprint(out:OutputChannel) end
    
  end
