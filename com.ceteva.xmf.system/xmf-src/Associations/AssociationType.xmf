parserImport XOCL;
import Associations;

context Associations
  @Class AssociationType extends Class
  
    @Attribute sourceType  : Class end
    @Attribute targetType  : Class end
    @Attribute sourceLevel : Element end
    @Attribute targetLevel : Element end
    @Attribute sourceMult  : Element end
    @Attribute targetMult  : Element end
    
    @Constructor(name, sourceType, targetType) // ! 
      self.addParent(Associations::AbstractAssociation)
    end
    
  end