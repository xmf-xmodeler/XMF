parserImport XOCL;

import Associations;

context Associations

  @Class MandatoryOne extends Multiplicity
  
    //  An end with multiplicity 1 requires that the attribute
    //  is associated with exactly one instance of the appropriate 
    //  class.
    
    @Constructor() end
    
    @Operation typeExp(type)
      type
    end
    
    @Operation labelString():String
      ""
    end
    
  end